{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "markdown",
  "readme": "none",
  "name": "Noodl Runtime Documentation",
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Services", "API", "Models", "Nodes", "Utils", "*"],
  "sort": ["source-order"],
  "validation": {
    "invalidLink": true,
    "notDocumented": true
  },
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": true
  }
}
